local MyCustomUI = loadstring(game:HttpGet("https://raw.githubusercontent.com/leshk19029/testilka/refs/heads/main/library"))()

-- Создание окна
local window = MyCustomUI:CreateWindow({
    Name = "Мое Кастомное Меню",
    Keybind = Enum.KeyCode.RightShift
})

-- Создание секций
local mainSection = window:CreateSection("Основные настройки")
local visualSection = window:CreateSection("Визуальные настройки")

-- Добавление элементов
local toggle = window:CreateToggle(mainSection, {
    Name = "Включить функцию",
    Default = false,
    Callback = function(state)
        print("Функция:", state)
    end
})

local slider = window:CreateSlider(mainSection, {
    Name = "Скорость",
    Min = 0,
    Max = 100,
    Default = 50,
    Callback = function(value)
        print("Скорость:", value)
    end
})

local button = window:CreateButton(mainSection, {
    Name = "Активировать",
    Callback = function()
        print("Функция активирована!")
    end
})

-- Визуальные настройки
window:CreateToggle(visualSection, {
    Name = "Эффекты",
    Default = true,
    Callback = function(state)
        print("Эффекты:", state)
    end
})

window:CreateSlider(visualSection, {
    Name = "Яркость",
    Min = 0,
    Max = 100,
    Default = 75,
    Callback = function(value)
        print("Яркость:", value)
    end
})
