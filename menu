local Compkiller = loadstring(game:HttpGet('https://raw.githubusercontent.com/4lpaca-pin/CompKiller/refs/heads/main/src/source.luau'))();

-- Создание окна с темным фоном
local Window = Compkiller.new({
    Name = "SPEED MENU",
    Keybind = "RightAlt",
    Logo = "rbxassetid://120245531583106",
    Scale = Compkiller.Scale.Window,
    TextSize = 15,
});

-- Создание вкладки
local SpeedTab = Window:DrawTab({
    Name = "Speed",
    Icon = "zap", -- Иконка молнии
    EnableScrolling = true
});

-- Создание секции с темным фоном
local SpeedSection = SpeedTab:DrawSection({
    Name = "Speed Controls",
    Position = 'left'
});

-- Добавление переключателя Speed Boost
local SpeedToggle = SpeedSection:AddToggle({
    Name = "Speed Boost",
    Flag = "Speed_Boost_Toggle",
    Default = false,
    Callback = function(enabled)
        if enabled then
            print("Speed Boost активирован!")
            -- Здесь будет код активации увеличения скорости
        else
            print("Speed Boost деактивирован!")
            -- Здесь будет код деактивации увеличения скорости
        end
    end,
});

-- Добавление слайдера для настройки скорости
local SpeedSlider = SpeedSection:AddSlider({
    Name = "Speed Value",
    Min = 0,
    Max = 25,
    Default = 16,
    Round = 0,
    Flag = "Speed_Value_Slider",
    Callback = function(value)
        print("Установлена скорость:", value)
        -- Здесь будет код установки значения скорости
    end
});

-- Добавление вспомогательного текста
SpeedSection:AddParagraph({
    Title = "Информация",
    Content = "Speed Boost: Включает/выключает ускорение\nSpeed Value: Устанавливает значение скорости (0-25)"
});
