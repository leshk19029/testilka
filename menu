local Zypher = loadstring(game:HttpGet('https://raw.githubusercontent.com/leshk19029/testilka/refs/heads/main/library'))()

-- Create the UI
local ui = Zypher.new("My Menu", Enum.KeyCode.RightShift, Color3.fromRGB(40, 40, 40))

-- Create Main tab
local mainTab = ui:CreateTab("Main")

-- Create Anti-Effects toggle, enabled by default
local antiEffectsToggle = ui:CreateToggle(mainTab, "Anti-Effects", true, function(value)
    print("Anti-Effects is now: " .. tostring(value))
    -- Add your Anti-Effects logic here
    if value then
        -- Code to disable effects (e.g., remove screen effects, post-processing, etc.)
        -- Example: game.Lighting.ColorCorrection.Enabled = false
    else
        -- Code to enable effects
        -- Example: game.Lighting.ColorCorrection.Enabled = true
    end
end)

-- Optional: Add a label for context
ui:CreateLabel(mainTab, "Toggle Anti-Effects to disable visual effects")

-- Show the UI immediately
ui:Toggle()
