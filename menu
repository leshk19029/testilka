local MyLibrary = loadstring(game:HttpGet("https://raw.githubusercontent.com/leshk19029/testilka/refs/heads/main/library"))()

local UI = MyLibrary:Create({
    Name = "МОЯ БИБЛИОТЕКА",
    Keybind = "RightShift"
})

local MainTab = UI:CreateTab("Главная")
local CombatTab = UI:CreateTab("Бой")

-- Добавляем элементы
local Toggle1 = MainTab:AddToggle({
    Name = "Включить функции",
    Default = false,
    Callback = function(value)
        print("Toggle:", value)
    end
})

local Button1 = MainTab:AddButton({
    Name = "Тестовая кнопка",
    Callback = function()
        print("Кнопка нажата!")
    end
})

local Slider1 = MainTab:AddSlider({
    Name = "Скорость",
    Min = 0,
    Max = 100,
    Default = 50,
    Round = 1,
    Callback = function(value)
        print("Скорость:", value)
    end
})
